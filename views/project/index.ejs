<Section class="project-content-home" id="project">
  <div class="project-title"><h1>Some of my projects</h1></div>
  <section class="card-list">
    <% for (let project of projects) {%>
    <a class="card" href="/project/<%= project._id %>">
      <header class="card-header">
        <h2><%- project.title %></h2>
        <p><%- project.breif %></p>
      </header>
      <div class="author-name">
        <div class="author-name-prefix">Author</div>
        Lu Chen
      </div>
      <div class="image">
        <img src="/svg/avatar.jpg" alt="lol" />
      </div>
    </a>
    <% } %>
  </section>
</Section>
